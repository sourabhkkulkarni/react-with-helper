  "scripts": {
    "start": "env-cmd -f .env.prod react-scripts start",
    "build": "env-cmd -f .env.prod react-scripts build && rm -rf sreunifiedconfiguration && mv build sreunifiedconfiguration",
    "build:local-win": "env-cmd -f .env.prod react-scripts build && DEL /S /Q sreunifiedconfiguration && move build sreunifiedconfiguration",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --testResultsProcessor ./node_modules/jest-sonar-reporter",
    "eject": "react-scripts eject"
  },
